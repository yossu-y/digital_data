<div class="container px-0">
  <div class="row">
    <div class="col-11 col-md-12 col-sm-12 mx-auto px-0 my-5 list">
      <% @posts.each do |post| %>
        <div class="card">
          <div class="card-img">
            <%= link_to post_path(post) do %>
              <%= image_tag post.get_image, class: "card-img" %>
            <% end %>
          </div>
          <div class="card-title text-center">
            ï¿¥<%= post.price.to_s(:delimited) %>
            <span class="bookmark" style="float: right" id="like_buttons_<%= post.id %>">
              <%= render "public/likes/btn", post: post %>
            </span>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div
